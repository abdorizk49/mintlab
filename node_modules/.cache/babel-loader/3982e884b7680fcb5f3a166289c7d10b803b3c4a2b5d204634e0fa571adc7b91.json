       </Form.Control.Feedback>\r\n                    </InputGroup>\r\n                </Col>\r\n                <Col md={4}>\r\n                    <InputGroup hasValidation>\r\n                        <Form.Select \r\n                            id=\"portfolioCategoryAdd\"\r\n                            name=\"portfolioCategoryAdd\"\r\n                            required\r\n                        >\r\n                        {categories.map((item) => (\r\n                            <option value={item.title} key={item.id}>\r\n                                {item.title}\r\n                            </option>\r\n                        ))}\r\n                        </Form.Select>\r\n                        <Form.Control.Feedback type=\"invalid\" className=\"text-start\">\r\n                            Please enter a valid Title.\r\n                        </Form.Control.Feedback>\r\n                    </InputGroup>\r\n                </Col>\r\n                <Col md={4}>\r\n                    <InputGroup hasValidation>\r\n                        <Form.Control\r\n                            id=\"portfolioDateAdd\"\r\n                            name=\"portfolioDateAdd\"\r\n                            required\r\n                            type=\"month\"\r\n                            placeholder=\"Project Date\"\r\n                        />\r\n                        <Form.Control.Feedback type=\"invalid\" className=\"text-start\">\r\n                            Please enter a valid Date.\r\n                        </Form.Control.Feedback>\r\n                    </InputGroup>\r\n                </Col>\r\n                <Col md={4}>\r\n                    <InputGroup hasValidation>\r\n                        <Form.Control \r\n                            id=\"portfolioClientAdd\"\r\n                            name=\"portfolioClientAdd\"\r\n                            required\r\n                            type=\"text\"\r\n                            placeholder=\"Project Client\"\r\n                        />\r\n                        <Form.Control.Feedback type=\"invalid\" className=\"text-start\">\r\n                            Please enter a valid Client.\r\n                        </Form.Control.Feedback>\r\n                    </InputGroup>\r\n                </Col>\r\n                <Col md={4}>\r\n                    <InputGroup hasValidation>\r\n                        <Form.Control\r\n                            id=\"portfolioUrlAdd\"\r\n                            name=\"portfolioUrlAdd\"\r\n                            required\r\n                            type=\"url\"\r\n                            placeholder=\"Project URL\"\r\n                        />\r\n                        <Form.Control.Feedback type=\"invalid\" className=\"text-start\">\r\n                            Please enter a valid URL.\r\n                        </Form.Control.Feedback>\r\n                    </InputGroup>\r\n                </Col>\r\n                <Col md={4}>\r\n                    <InputGroup hasValidation>\r\n                        <Form.Control\r\n                            id=\"portfolioImageAdd\"\r\n                            name=\"portfolioImageAdd\"\r\n                            required\r\n                            type=\"url\"\r\n                            placeholder=\"Project Image URL\"\r\n                        />\r\n                        <small>The best size is 600*750</small>\r\n                        <Form.Control.Feedback type=\"invalid\" className=\"text-start\">\r\n                            Please enter a valid URL.\r\n                        </Form.Control.Feedback>\r\n                    </InputGroup>\r\n                </Col>\r\n                <Col md={12}>\r\n                    <InputGroup hasValidation>\r\n                        <Form.Control \r\n                            as=\"textarea\"\r\n                            id=\"portfolioDescriptionAdd\"\r\n                  